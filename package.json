    "@types/express": "^4.17.21",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
