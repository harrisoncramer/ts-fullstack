{
  "name": "vue-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --cache src server",
    "start": "concurrently \"npm run start:vite\" \"npm run start:express\"",
    "start:vite": "vite",
    "start:express": "cd server && DEBUG=true PORT=3001 tsx --watch index.ts",
    "build": "npm run build:server && npm run build:client",
    "build:client": "vue-tsc -p src/tsconfig.json && vite build",
    "build:server": "tsc --build server/tsconfig.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "express": "^4.19.2",
    "normalize.css": "^8.0.1",
    "pinia": "^2.2.0",
    "vue": "^3.4.31",
    "vue-router": "^4.4.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "tsx": "^4.16.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.3.4",
    "vue-tsc": "^2.0.24"
  }
}
